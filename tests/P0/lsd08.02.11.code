program test1101;
var
  x : integer;
  y : integer;
  a : boolean;
  b : boolean;
begin
  x := 1;
  y := 2 + 3 * x; {y = 3 * 1 + 2 = 3 + 2 = 5}
  y := y - x;     {y = 4 - 1 =4}
  y := -y;        {y = -4}
  y := -x - y;    {y = -1 -(-4) = -1 + 4 = 3}
  write (y - x);
  b := true;
  b := not b;
  read y;
  a := false;
  a := a or b;
  write x;
  b := x = y;
  a := b and a;
  read x;
  b := x > y;
  a := x < y;
  write y
end.